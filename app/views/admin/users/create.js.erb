<% if @user.errors.blank? %>
  $('.modal-backdrop').remove();
  $('#newUserForm').modal('toggle');
  $('.users_body').append("<%= j render 'admin/users/user', user: @user %>");
  $('#success').show('slow');
  $('#success').html("User added successfully..");
<% else %>
  $('#user_errors').html("<%= j render 'admin/users/form_errors', user: @user %>");
<% end %>
